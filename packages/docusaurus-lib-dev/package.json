{
  "name": "docusaurus-lib-dev",
  "version": "1.0.0-beta.18",
  "description": "A Docusaurus plugin for live component demos with rehype processing and interactive code previews",
  "keywords": [
    "docusaurus",
    "plugin",
    "rehype",
    "component",
    "demo",
    "preview",
    "react",
    "mdx",
    "documentation"
  ],
  "license": "MIT",
  "author": {
    "name": "jay.kou",
    "email": "jay.kou@qq.com"
  },
  "exports": {
    "./components": {
      "import": "./es-components/index.js",
      "require": "./lib-components/index.js",
      "types": "./es-components/index.d.ts"
    },
    "./rehype": "./lib-rehype/index.js",
    "./styles": "./es-components/styles/index.css",
    ".": "./lib/index.js"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "es",
    "lib-components",
    "es-components",
    "lib-rehype",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "father build && npm run complie-less && npm run build-rehype && npm run build-components",
    "build-components": "cross-env BUILD_TYPE=components father build",
    "build-rehype": "cross-env BUILD_TYPE=rehype father build",
    "complie-less": "lessc ./src/components/styles/index.less ./src/components/styles/index.css",
    "doctor": "father doctor",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/?(*.)+(test).js"
    ]
  },
  "dependencies": {
    "@ant-design/icons-svg": "^4.4.2",
    "@babel/parser": "^7.27.0",
    "@babel/runtime": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@rollup/pluginutils": "^5.1.4",
    "file-system-cache": "2.4.7",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.1",
    "lodash": "^4.17.21",
    "prism-react-renderer": "^2.4.1",
    "rc-tabs": "^15.6.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-docgen-typescript": "2.4.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "upath": "^2.0.1",
    "vfile": "^6.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.0",
    "@babel/types": "^7.27.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.14.1",
    "@types/react": "^18.1.0",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18.1.0",
    "@types/unist": "^3.0.3",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "father": "^4.5.2",
    "jest": "^29.7.0",
    "less": "^4.3.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "packageManager": "pnpm@10.11.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
